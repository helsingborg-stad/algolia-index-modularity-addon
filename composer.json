{
    "name": "helsingborg-stad/algolia-index-modularity-addon",
    "description": "https://github.com/helsingborg-stad/algolia-index-modularity-addon",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Thulin",
            "email": "sebastian.thulin@helsingborg.se"
        }
    ],
    "scripts": {
        "test": "phpunit --no-coverage --testdox",
        "test:coverage": "XDEBUG_MODE=coverage phpunit --testdox",
        "lint": "vendor/bin/mago lint",
        "fix": "vendor/bin/mago lint --format-after-fix --fix",
        "format": "vendor/bin/mago fmt"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "AlgoliaIndexModularityAddon\\": "source/php/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "carthage-software/mago": "^1.0.0-rc.1"
    },
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true
        }
    }
}